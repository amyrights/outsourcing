<p id="notice"><%= notice %></p>

<h1>Company Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Headquarters country code</th>
      <th>Headquarters state code</th>
      <th>Headquarters city</th>
      <th>Website</th>
      <th>Twitter</th>
      <th>Linkedin</th>
      <th>Facebook</th>
      <th>Summary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_profiles.each do |company_profile| %>
      <tr>
        <td><%= company_profile.headquarters_country_code %></td>
        <td><%= company_profile.headquarters_state_code %></td>
        <td><%= company_profile.headquarters_city %></td>
        <td><%= company_profile.website %></td>
        <td><%= company_profile.twitter %></td>
        <td><%= company_profile.linkedin %></td>
        <td><%= company_profile.facebook %></td>
        <td><%= company_profile.summary %></td>
        <td><%= link_to 'Show', company_profile %></td>
        <td><%= link_to 'Edit', edit_company_profile_path(company_profile) %></td>
        <td><%= link_to 'Destroy', company_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company Profile', new_company_profile_path %>
